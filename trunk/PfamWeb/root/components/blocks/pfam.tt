
[%# requires a pfam object called "pfam" %]

<!-- start pfam block -->

<div class="block" id="pfamBlock">
  <div class="handle">
    <h1>Pfam entry: <em>[% pfam.pfamA_id %]</em> ([% pfam.pfamA_acc %])</h1>
  </div>
  <div class="blockContent[% IF NOT open %] closed[% END %]">
	<p>acc = [% pfam.pfamA_acc %]</p>
	<p>id  = [% pfam.pfamA_id %]</p>
	<p>description  = [% pfam.description %]</p>
	<p>comment  = [% pfam.comment %]</p>
  </div>
</div>

<!-- end pfam block -->
