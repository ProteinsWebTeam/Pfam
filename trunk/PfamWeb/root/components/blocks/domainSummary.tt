
<!-- start domain architectures block -->

<div class="block" id="domainsBlock">
  <div class="handle">
    <h1>Domain organisation</h1>
  </div>
  <div class="blockContent[% IF NOT open %] closed[% END %]">
    [% FOREACH image IN images.each_image %]
	  [% CALL image.print_image %]
      <div class="domain">
        <img src="/[% image.file_location %]" alt=""/>
      </div>
	[% END %]
  </div>
</div>

<!-- end domain architectures block -->