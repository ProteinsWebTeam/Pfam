#-------------------------------------------------------------------------------
# specify required stylesheets and javascript files 

PROCESS "components/require.tt";

# using styles from the help pages, plus the common styles for forms
requirements.css.push( "help.css" );
requirements.css.push( "forms.css" );
requirements.cssIeAll.push( "forms_all_ie.css" );

#-------------------------------------------------------------------------------

%]

<!-- start protein search block -->

<div class="block" id="searchProteinBlock">
  <div class="handle">
    <h1>Protein name or sequence search</h1>
  </div>
  <div class="blockContent">

    <div id="psfd" class="formWrapper">

      <h3>Protein entry search</h3>
  
      <form method="get" 
            name="proteinSearchForm"
            id="proteinSearchForm"
            action="[% c.uri_for( "/seqsearch/unp" ) %]">
  
        <span class="row">
          <label for="unp">UniProt ID or accession</label>
          <input type="text" size="20" maxlength="40" name="unp" id="unp" />
        </span>
  
        <span class="row">
          <input class="submit"  type="submit" name="submit" value="Submit" />
          <input class="reset"   type="reset"  name="reset"  value="Reset" />
          <input class="example" type="button" value="Example" onclick="$('unp').value=exUnp" />
        </span>
  
      </form>
  
      <hr />
  
      <h3>Single sequence searches</h3>
  
      <form method="get" 
            name="proteinSeqSearchForm"
            id="proteinSeqSearchForm"
            action="[% c.uri_for( "/seqsearch/seq" ) %]">
  
        <span class="row">
          <label for="seq">Sequence</label>
          <textarea cols="60" rows="10" name="seq" id="seq"></textarea>
        </span>
  
        <span class="row">
          <label for="seqOpts">Search options</label>
          <select name="seqOpts" id="seqOpts">
            <option value="both">Both global &amp; fragment</option>
            <option value="ls">Global</option>
            <option value="fs">Fragment</option>
          </select>
        </span>
  
        <span class="row">
          <label for="evalue">E-value cut-off level</label>
          <input type="text" size="10" maxlength="10" value="1.0" name="evalue" id="evalue" />
        </span>
  
        <span class="row">
          <input class="submit"  type="submit" name="submit" value="Submit" />
          <input class="reset"   type="reset"  name="reset"  value="Reset" />
          <input class="example" type="button" value="Example" onclick="$('seq').value=exSeq" />
        </span>
  
      </form>
  
      <h3>Batch searches</h3>
  
      <form method="get" 
            name="proteinBatchSearchForm"
            id="proteinBatchSearchForm"
            action="[% c.uri_for( "/seqsearch/batch" ) %]">
  
        <span class="row">
          <label for="seq">Sequence</label>
          <input type="file" size="20" name="batchSeq" id="batchSeq" />
        </span>
  
        <span class="row">
          <label for="seqOpts">Search options</label>
          <select name="seqOpts" id="seqOpts">
            <option value="both">Both global &amp; fragment</option>
            <option value="ls">Global</option>
            <option value="fs">Fragment</option>
          </select>
        </span>
  
        <span class="row">
          <label for="email">Email address</label>
          <input type="text" size="20" maxlength="100" name="email" id="email" />
        </span>
  
        <span class="row">
          <input class="submit"  type="submit" name="submit" value="Submit" />
          <input class="reset"   type="reset"  name="reset"  value="Reset" />
        </span>
  
      </form>
  
    </div>

  </div>
</div>

