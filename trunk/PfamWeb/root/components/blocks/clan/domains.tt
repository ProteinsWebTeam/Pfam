<!-- start domain organisation block -->

<div class="block" id="clanDomainsBlock">
  <div class="handle">
    <h1>Domain organisation</h1>
  </div>
  <div class="blockContent">

	<div id="clanGraphicsHolder">
      <p id="cgph">
        <img src="/images/loading.gif" alt="loading..." />
        Loading domain graphics...
      </p>
    </div> <!-- end of clanGraphicsHolder -->

  </div> <!-- end of blockContent -->

</div> <!-- end of clanDomainsBlock -->

<script type="text/javascript">
  loadOptions.cg.uri    = "[% c.uri_for( "/clan/graphics" ) %]";
  loadOptions.cg.params = "id=[% clan.clan_id %]";
</script>

<!-- end domain organisation block -->
